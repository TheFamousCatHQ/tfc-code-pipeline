# Example environment variables
# Copy this file to .env and modify as needed
# These API keys are required for various AI services used by the pipeline

# Google Gemini API key
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Anthropic API key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Sonar token for code analysis
SONAR_TOKEN=your_sonar_token_here

# Aider configuration
AIDER_MODEL=claude-3-7-sonnet-latest
AIDER_WEAK_MODEL=claude-3-5-haiku-latest
AIDER_YES_ALWAYS=true
AIDER_DETECT_URLS=false
AIDER_ANALYTICS_DISABLE=true
AIDER_CHECK_UPDATE=false

# Debug mode
DEBUG=False
